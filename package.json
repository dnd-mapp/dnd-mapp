{
  "name": "@dnd-mapp/dnd-mapp",
  "description": "The primary interface and gateway for the D&D Mapp platform.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "NoNamer777",
  "homepage": "https://github.com/dnd-mapp/dnd-mapp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnd-mapp/dnd-mapp.git"
  },
  "bugs": {
    "url": "https://github.com/dnd-mapp/dnd-mapp/issues"
  },
  "packageManager": "pnpm@10.29.3",
  "type": "module",
  "engines": {
    "node": ">=24.13.1",
    "pnpm": ">=10.29.3"
  },
  "scripts": {
    "gen:ssl-cert": "mkcert -key-file ssl-key.pem -cert-file ssl-cert.pem localhost.www.dndmapp.dev localhost 127.0.0.1",
    "start": "pnpm exec ng serve",
    "build": "pnpm exec ng build",
    "build-dev": "pnpm exec ng build -c development",
    "docker:build": "docker buildx bake -f .docker/docker-bake.hcl app",
    "format:check": "pnpm exec prettier -l .",
    "format:write": "pnpm exec prettier -w .",
    "lint": "pnpm exec ng lint",
    "stylelint": "pnpm exec stylelint \"**/*.css\""
  },
  "dependencies": {
    "@angular/common": "~21.1",
    "@angular/compiler": "~21.1",
    "@angular/core": "~21.1",
    "@angular/forms": "~21.1",
    "@angular/platform-browser": "~21.1",
    "@angular/router": "~21.1",
    "rxjs": "~7.8",
    "tslib": "~2.8"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~21.2",
    "@angular/build": "~21.1",
    "@angular/cli": "~21.1",
    "@angular/compiler-cli": "~21.1",
    "@eslint/js": "~9.39",
    "@tailwindcss/postcss": "~4.1",
    "angular-eslint": "~21.2",
    "eslint": "~9.39",
    "eslint-config-prettier": "~10.1",
    "postcss": "~8.5",
    "prettier": "~3.8",
    "prettier-plugin-organize-imports": "~4.3",
    "stylelint": "~17.1",
    "stylelint-config-standard": "~40.0",
    "tailwindcss": "~4.1",
    "typescript": "~5.9",
    "typescript-eslint": "~8.55"
  }
}
